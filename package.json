    "@sap/cds": "^7",
    "express": "^4",
    "@sap/cds-hana": "^2",
    "@sap/xssec": "^3",
    "passport": "^0"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^7",
    "@sap/ux-specification": "^1.108.12"
  },
  "scripts": {
    "start": "cds-serve",
    "hybrid": "cds watch --profile hybrid",
    "build": "mbt build -t gen --mtar mta.tar",
